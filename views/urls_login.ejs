<body>

<header>
    <% include ./partials/_header %>
</header>



    <form class="form" action="/register" method="POST">
      

        <label for="email">Enter Email</label>
        <input class="form-control" type="email" name="email" placeholder="enter email" style="width: 400px; margin: 1em">

